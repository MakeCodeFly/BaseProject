<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


    <TextView
        android:text="HomeFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <com.zoujuequn.baseproject.widget.MakeCodeFlyButton
        android:id="@+id/am_chat"
        android:layout_margin="@dimen/dp_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:radius="5dp"
        app:round="true"
        app:normalStrokeColor="#000999"
        app:normalBackgroundColor="@color/colorAccent"
        app:pressedBackgroundColor="@color/colorPrimary"
        app:strokeDashWidth="2dp"

        />

</LinearLayout>